{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "www",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "sw.js",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "no-cache"
        } ]
      }
    ]
  }
}
